<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".splash">

  <ImageView
      android:id="@+id/logoImg"
      android:layout_width="@dimen/_190sdp"
      android:layout_height="@dimen/_164sdp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/aa" />

  <TextView
      android:id="@+id/owntwo"
      android:layout_width="@dimen/_100sdp"
      android:layout_height="@dimen/_25sdp"
      android:gravity="center"
      android:text="Amritesh"
      android:textSize="@dimen/_15sdp"
      android:fontFamily="@font/spartan_thin"
      android:textStyle="bold"
      android:textColor="@color/black"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.964" />

  <TextView
      android:id="@+id/ownone"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/spartan_thin"
      android:gravity="center"
      android:text="From"
      android:textColor="@color/black"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="@+id/owntwo"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.94" />


</androidx.constraintlayout.widget.ConstraintLayout>